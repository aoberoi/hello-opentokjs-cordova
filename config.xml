<?xml version='1.0' encoding='utf-8'?>
<widget id="me.aoberoi.cordova.helloopentokjs" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloOpenTokJS</name>
    <description>
      An example app for the OpenTokJS cordova plugin
    </description>
    <author email="aoberoi@gmail.com" href="https://aoberoi.me">
        Ankur Oberoi
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="4.0.2" />
    <engine name="browser" spec="3.6.0" />
    <engine name="ios" spec="3.8.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-device" spec="^1.0.1" />
    <plugin name="cordova-plugin-opentokjs" spec="^0.1.2" />
</widget>
